' Create Excel Object
Set objExcel = CreateObject("Excel.Application")
objExcel.Visible = False ' Set to True if you want to see the Excel window

' Path to the Excel file
excelPath = "C:\Path\To\Your\File.xlsx"

' Open or create a new workbook
On Error Resume Next
Set objWorkbook = objExcel.Workbooks.Open(excelPath)
If Err.Number <> 0 Then
    Set objWorkbook = objExcel.Workbooks.Add()
    Err.Clear
End If
On Error GoTo 0

' Get the first worksheet
Set objSheet = objWorkbook.Sheets(1)

' Example: array of keys to write
arrKeys = Array("Name", "Age", "City", "Gender")

' Row and column to start writing
row = 1
col = 1

' Write each key in a row
For i = 0 To UBound(arrKeys)
    objSheet.Cells(row, col + i).Value = arrKeys(i)
Next

' Save the file
objWorkbook.SaveAs excelPath
objWorkbook.Close False
objExcel.Quit

' Release the objects
Set objSheet = Nothing
Set objWorkbook = Nothing
Set objExcel = Nothing