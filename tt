import java.text.SimpleDateFormat;
import java.util.*;

public class SortByDateInString {
    public static void main(String[] args) {
        // Sample list of strings
        List<String> stringsWithDates = Arrays.asList(
            "Sep 8 60.00 about gdkd",
            "Jan 15 45.00 about abc",
            "Dec 25 100.00 about xyz",
            "Mar 3 30.00 about lmn"
        );

        // Define the date format
        SimpleDateFormat dateFormat = new SimpleDateFormat("MMM d", Locale.ENGLISH);

        // Sort the list
        stringsWithDates.sort(Comparator.comparing(s -> {
            try {
                return dateFormat.parse(s.split(" ", 3)[0] + " " + s.split(" ", 3)[1]);
            } catch (Exception e) {
                throw new RuntimeException(e); // Handle parse exceptions
            }
        }));

        // Print the sorted list
        stringsWithDates.forEach(System.out::println);
    }
}